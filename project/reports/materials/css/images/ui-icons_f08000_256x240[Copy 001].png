INSERT INTO `okb_forms` (`ID`, `ORD`, `NAME`, `FILE`, `ID_formgroups`, `PATTERN`, `HLP`, `GROUPID`, `SHOWALL`, `USEFILE`) VALUES (48, 'Управление персоналом', 'Ресурсы - уволенные', '', 0, '{{notprinted}}\n<div class=&#39;links&#39;>\n<a href=&#39;index.php?do=show&#38;formid=44&#39;>Работающие</a>\n</div>\n{{/notprinted}}\n\n<H2>Ресурсы</H2>\n\n<?php \n\n$p2 = &#39;NAME&#39;; \n$p3 = &#39;&#39; ;\n\nif( isset( $_GET[&#39;p2&#39;] ) )\n  $p2 = $_GET[&#39;p2&#39;]; \n\nif( isset( $_GET[&#39;p3&#39;] ) )\n  $p3 = $_GET[&#39;p3&#39;]; \n  \n$str=&quot;order by binary(`$p2`)&quot;; \n\n$str .= &quot; $p3&quot;;\n\necho &quot;<script>var p2 = &#39;$p2&#39;; p3 = &#39;$p3&#39;; </script>&quot;;\n\n?>\n\n{{item}}\n44, false, false, true, true, &quot;(TID=&#39;1&#39;) and (ID>0)&quot;, &quot;&quot;, &quot;$str&quot;, &quot;&quot;\n{{/item}}\n\n<script type=&#39;text/javascript&#39;>\n\nvar p1 = getUrlVars()[&#39;p1&#39;];\n\nif (p1==&#39;1&#39;) {\nhistory.replaceState(0, &quot;New page title&quot;, &quot;index.php?do=show&#38;formid=44&quot;);\nalert (&#39;Этот аккаунт уже используется &#39;);\n}\nfunction getUrlVars() {\n   var vars = {};\n   var parts = window.location.href.replace(/[?&#38;]+([^=&#38;]+)=([^&#38;]*)/gi, function(m,key,value) {\n   vars[key] = value;\n   });\n   return vars;\n}\n\nfunction change_sort_type()\n{\n  var id = $( this ).attr(&#39;id&#39;) ;\n  var val = $( this ).attr(&#39;val&#39;) ;\n  var field, sort = &#39;&#39;;\n  \n  switch( id )\n  {\n    case &#39;res_fio&#39;      : field = &#39;NAME&#39;; break ;\n    case &#39;res_accept&#39;   : field = &#39;DATE_FROM&#39;; break ;\n    case &#39;res_dismiss&#39;  : field = &#39;DATE_TO&#39;; break ; \n  }\n\n  switch( val )\n  {\n    case &#39;up&#39;   : sort = &#39;DESC&#39; ; \n                  break ;\n    \n    case &#39;na&#39;   :                   \n    case &#39;down&#39; : sort = &#39;ASC&#39;; \n                  break ;\n  }\n\n  location.href = &#39;/index.php?do=show&#38;formid=44&#38;p2=&#39; + field + &#39;&#38;p3=&#39; + sort;\n  \n}\n\n\nfunction change_images()\n{\n  var keys = getUrlVars() ;\n  \n  var field = keys[&#39;p2&#39;];\n  var sort = keys[&#39;p3&#39;];  \n  var img = &#39;&#39; ;\n  var dir = &#39;&#39; ;\n  \n  \n  if( field == undefined || sort  == undefined )\n    return ;\n  \n  switch( sort )\n  {\n\n      case &#39;ASC&#39;   : img = &#39;project/img5/1.gif&#39;; \n                     dir = &#39;up&#39;; \n                     break ;\n      case &#39;DESC&#39;  : img = &#39;project/img5/0.gif&#39;; \n                     dir = &#39;down&#39;; \n                     break ; \n  }\n \n  switch( field )\n  {\n    case &#39;NAME&#39;: \n                  $(&#39;img#res_fio&#39;).attr(&#39;src&#39;, img ).attr(&#39;val&#39;, dir );\n                  $(&#39;img#res_accept&#39;).attr(&#39;src&#39;, &#39;project/img5/c1.gif&#39;).attr(&#39;val&#39;, &#39;na&#39; );                  \n                  $(&#39;img#res_dismiss&#39;).attr(&#39;src&#39;, &#39;project/img5/c1.gif&#39;).attr(&#39;val&#39;, &#39;na&#39; );                                    \n                  break;\n\n    case &#39;DATE_FROM&#39;: \n                  $(&#39;img#res_accept&#39;).attr(&#39;src&#39;, img ).attr(&#39;val&#39;, dir );\n                  $(&#39;img#res_fio&#39;).attr(&#39;src&#39;, &#39;project/img5/c1.gif&#39;).attr(&#39;val&#39;, &#39;na&#39; );\n                  $(&#39;img#res_dismiss&#39;).attr(&#39;src&#39;, &#39;project/img5/c1.gif&#39;).attr(&#39;val&#39;, &#39;na&#39; );\n                  break;\n\n    case &#39;DATE_TO&#39;: \n                  $(&#39;img#res_dismiss&#39;).attr(&#39;src&#39;, img ).attr(&#39;val&#39;, dir );\n                  $(&#39;img#res_fio&#39;).attr(&#39;src&#39;, &#39;project/img5/c1.gif&#39;).attr(&#39;val&#39;, &#39;na&#39; );                  \n                  $(&#39;img#res_accept&#39;).attr(&#39;src&#39;, &#39;project/img5/c1.gif&#39;).attr(&#39;val&#39;, &#39;na&#39; ); \n                  break;\n  }\n}\n\n// Действия после загрузки страницы : \n$( function()\n{\n  $(&#39;img.res_sort_img&#39;).click( change_sort_type );\n  change_images();\n});\n\n</script>\n<style>\ntd.Field {vertical-align: middle;}\n</style>', '', 0, 1, 0);
